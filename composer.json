{
  "name": "yiimaker/yii2-email-templates",
  "description": "Extension for create and use email templates across your site dashboard",
  "keywords": ["yii2", "module", "email", "templates"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/yiimaker/yii2-email-templates",
    "issues": "https://github.com/yiimaker/yii2-email-templates/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "^2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yii2deman/yii2deman-language-provider": "dev-master",
    "vova07/yii2-imperavi-widget": "~1.2"
  },
  "require-dev": {
    "codeception/codeception": "~2.2",
    "codeception/verify": "*",
    "codeception/specify": "*"
  },
  "authors": [
    {
      "name": "Vladimir Kuprienko",
      "email": "vldmr.kuprienko@gmail.com",
      "role": "developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "ymaker\\email\\templates\\": "src",
      "ymaker\\email\\templates\\tests\\": "tests"
    }
  }
}
{
  "name": "yiimaker/yii2-email-templates",
  "description": "Extension for creating of email templates and manage using your site dashboard",
  "keywords": ["yii2", "module", "gii", "email", "templates"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/yiimaker/yii2-email-templates",
    "issues": "https://github.com/yiimaker/yii2-email-templates/issues"
  },
  "authors": [
    {
      "name": "Vladimir Kuprienko",
      "email": "vldmr.kuprienko@gmail.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "^2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yii2deman/yii2deman-language-provider": "~0.2",
    "vova07/yii2-imperavi-widget": "~1.3.1"
  },
  "require-dev": {
    "yiisoft/yii2-gii": "~2.0.0",
    "codeception/codeception": "~2.0",
    "codeception/verify": "*",
    "codeception/specify": "*"
  },
  "autoload": {
    "psr-4": {
      "ymaker\\email\\templates\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ymaker\\email\\templates\\tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.2.x-dev"
    }
  },
  "scripts": {
    "test": [
      "codecept build",
      "codecept run"
    ]
  }
}